<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="no.uio.inf5750.assignment2.model.Course" table="students">

		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>

		<property name="courseCode" not-null="true" unique="true"
			column="course_code" type="string" />
	
		<!-- Bidirectional many-to-many -->
		<set name="attendants" table="course_students" inverse="true">
			<key column="course_id" />
			<many-to-many column="student_id"
				class="no.uio.inf5750.assignment2.model.Student" />
		</set>




	</class>
</hibernate-mapping>